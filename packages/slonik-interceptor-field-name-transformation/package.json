      "--import=tsimp"
    ]
  },
  "dependencies": {
    "camelcase": "^8.0.0"
  },
  "description": "Transforms Slonik query result field names.",
  "devDependencies": {
    "@slonik/eslint-config": "workspace:^",
    "ava": "^6.4.1",
    "eslint": "9.39.2",
    "slonik": "^48.10.3",
    "tsimp": "^2.0.12",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.55.0"
  },
  "engines": {
    "node": ">=24"
  },
  "keywords": [
    "postgresql",
    "interceptor",
    "format"
  ],
  "license": "BSD-3-Clause",
  "main": "./dist/index.js",
  "name": "slonik-interceptor-field-name-transformation",
  "peerDependencies": {
    "slonik": ">=45.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gajus/slonik"
  },
  "scripts": {
    "build": "rm -fr ./dist && tsc",
    "lint": "eslint ./src && tsc --noEmit",
    "test": "ava --verbose --serial"
  },
  "type": "module",
  "types": "./dist/index.d.ts",
  "version": "48.10.3"
}
