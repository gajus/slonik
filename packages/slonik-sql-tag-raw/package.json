  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "files": [
      "src/**/*.test.ts"
    ],
    "nodeArguments": [
      "--import=tsimp"
    ]
  },
  "dependencies": {
    "@slonik/sql-tag": "^48.6.0",
    "roarr": "^7.21.1"
  },
  "description": "Logs Slonik queries.",
  "devDependencies": {
    "@slonik/eslint-config": "workspace:^",
    "ava": "^6.3.0",
    "eslint": "9.28.0",
    "slonik": "^48.6.0",
    "tsimp": "^2.0.12",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.33.0"
  },
  "engines": {
    "node": ">=24"
  },
  "keywords": [
    "postgresql",
    "interceptor",
    "logging"
  ],
  "license": "BSD-3-Clause",
  "main": "./dist/index.js",
  "name": "slonik-sql-tag-raw",
  "peerDependencies": {
    "slonik": ">=45.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gajus/slonik"
  },
  "scripts": {
    "build": "rm -fr ./dist && tsc",
    "lint": "eslint ./src && tsc --noEmit",
    "test": "ava --verbose --serial"
  },
  "type": "module",
  "types": "./dist/index.d.ts",
  "version": "48.6.0"
}
